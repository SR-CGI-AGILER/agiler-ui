{{yield}}

  This is the schedule view:
  {{#paper-list}}
    {{#paper-subheader}}scheduled tasks{{/paper-subheader}}
    {{#each tasks as |category|}}
      {{#paper-item class="md-1-line" onClick=(action "selectCategory" category)}}
        <div class="md-list-item-text">
          <p>{{category.owner}}</p>
        </div>
        <div class="md-secondary-container">
          {{paper-icon "navigate_next"}}
        </div>
      {{/paper-item}}
    {{/each}}
  {{/paper-list}}
