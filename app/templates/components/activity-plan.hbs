{{yield}}
  {{#paper-list}}
    {{#paper-subheader}}Activity Plan{{/paper-subheader}}
    {{#each tasks as |category|}}
      {{#paper-item class="md-1-line"}}
        <div class="md-list-item-text">
          <p>{{category.text}}</p>
        </div>
        <div class="md-secondary-container">
          {{paper-icon "navigate_next"}}
        </div>
      {{/paper-item}}
    {{/each}}
   
  {{/paper-list}}
  {{#paper-button iconButton=true onClick = (action "saveToDb")}}
    {{paper-icon "publish"}}
  {{/paper-button}} 
