{{#paper-list}}
{{#each tasks as |category|}}
{{#paper-item class="md-1-line"}}
<div class="md-list-item-text">
    <p>{{category.owner}}</p>
</div>
<div class="md-secondary-container">
    {{#paper-button iconButton=true onClick=(action "selectCategory" category)}}
            {{paper-icon "navigate_next"}}
        {{/paper-button}}
</div>
{{/paper-item}}
 {{#if showMultiComp}}
        {{#multi-task-select}}
        {{/multi-task-select}}
    {{/if}}
{{/each}}
{{/paper-list}}