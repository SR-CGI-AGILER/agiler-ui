{{#paper-list}}

{{#each backlogTasks as |category|}}
    {{#paper-item class="md-1-line"}}
    <div class="md-list-item-text">
        <p>{{category.text}}</p>
    </div>
    <div class="md-secondary-container">
        {{#paper-button iconButton=true onClick=(action "selectTask" category)}}
            {{paper-icon "navigate_next"}}
        {{/paper-button}}
        
    </div>
    {{/paper-item}}
    {{#liquid-if showMultiComp}}
        {{#paper-item}}
            {{#multi-task-select}}
            {{/multi-task-select}}
        {{/paper-item}}
    {{/liquid-if}}
{{/each}}
{{/paper-list}}

    
    